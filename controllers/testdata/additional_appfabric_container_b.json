{
    "image":"gcr.io/google-samples/hello-app:2.0",
    "name":"test-appfabric-container-b",
    "readinessProbe":{
      "exec":{
        "command":[
          "ls"
        ]
      },
      "failureThreshold":3,
      "periodSeconds":10,
      "successThreshold":1,
      "timeoutSeconds":1
    },
    "livenessProbe":{
      "httpGet":{
        "path":"/_authn/200",
        "port":3000,
        "scheme":"HTTP"
      },
      "initialDelaySeconds":30,
      "timeoutSeconds":5
    },
    "ports":[
      {
        "containerPort":3000,
        "name":"test-port",
        "protocol":"test-protocol"
      }
    ]
  }